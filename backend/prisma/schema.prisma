generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String @unique
  fullname String
  password String
  username String @unique
  StudentData StudentData[]
}

model StudentData {
  id         String @id @default(auto()) @map("_id") @db.ObjectId
  studentname String
  studytime   Int
  failures    Int
  absences    Int
  schoolsup   Boolean
  paid        Boolean
  health      Int
  G1        Float
  G2        Float
  G3        Float
  average    Float
  prediction Float
  teacher  User @relation(fields: [teacherId], references: [id])
  teacherId String @db.ObjectId
}
