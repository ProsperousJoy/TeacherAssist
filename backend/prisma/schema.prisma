generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model TeacherAssist {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String @unique
  fullname String
  password String
  username String @unique
}

model User {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}
